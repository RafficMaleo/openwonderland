<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <servlet>
        <servlet-name>JnlpDownloadServlet</servlet-name>
        <servlet-class>org.jdesktop.deployment.jnlp.servlet.JnlpDownloadServlet</servlet-class>
        <init-param>
            <param-name>jnlpSubstitutionHandlerClass</param-name>
            <param-value>org.jdesktop.lg3d.wonderland.servlet.WonderlandSubstitutionHandler</param-value>
        </init-param>
        </servlet>
    <servlet-mapping>
        <servlet-name>JnlpDownloadServlet</servlet-name>
        <url-pattern>/app/*</url-pattern>
    </servlet-mapping>
</web-app>
