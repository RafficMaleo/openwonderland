<test xmlns="http://jtrunner.sourceforge.net"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://jtrunner.sourceforge.net file:./config/jtr.xsd" 
    runs="0">

    <!-- JTR components factories -->
    <factories>
        <factory key="IAssignmentPolicyFactory" fqn="jtr.assigner.impl.DefaultAssignmentPolicyFactory" />
        <factory key="IOutcomeFactory" fqn="jtr.test.impl.DefaultOutcomeFactory" />
        <factory key="IWsHelperFactory" fqn="org.jdesktop.wonderland.testharness.slave.jtr.dummy.JTRNullWsHelperFactory" />
        <factory key="ITestCompletionListener" fqn="org.jdesktop.wonderland.testharness.slave.jtr.dummy.JTRNullTestCompletionListener" />
        <factory key="ITestResultDisplayer" fqn="org.jdesktop.wonderland.testharness.slave.jtr.dummy.JTRNullResultsDisplayer" />
        <factory key="ITestResultsExporter" fqn="org.jdesktop.wonderland.testharness.slave.jtr.dummy.JTRNullResultsExporter" />
    </factories>

    <!-- Nodes for JTR EDiT test sessions -->
    <nodes>
        <node host="localhost" port="2000"/>
    </nodes>
    
    <!-- Runners Definitions -->
    <runner runs="1">
        <runner-fqn>org.jdesktop.wonderland.testharness.slave.jtr.JTRSlaveRunner</runner-fqn>
        <instance-count>4</instance-count>
        <parameters-assignment-policy>cyclic</parameters-assignment-policy>
        <parameters>
            <param name="sleepTime" value="100"/>
            <param name="hostname" value="localhost"/>
            <param name="port" value="43210"/>
        </parameters>
    </runner>
    
    <!-- Enterprise Configurations -->
    
    <!-- JMS Configurations -->
    
    <!-- WebServices Bindings -->
    
</test>
